{
  "name": "electron-devdocs",
  "description": "an electron-vue app for devdocs",
  "scripts": {
    "dev": "node tasks/runner.js",
    "both-dev": "nodemon --delay 5 -w app/src/main tasks/runner.js",
    "build": "npm run pack && build",
    "build:dir": "npm run pack && build --dir",
    "dist": "npm run pack && build",
    "release": "npm run pack && build",
    "pack-all": "build -mwl",
    "package-all": "electron-packager . --all",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter app test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix app test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack -p --progress --colors --config webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack -p --progress --colors --config webpack.renderer.config.js",
    "re-npm": "rimraf node_modules && cnpm install",
    "test": "npm run unit && npm run e2e",
    "unit": "cross-env BABEL_ENV=testing-unit karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix && cd app && npm install"
  },
  "build": {
    "productName": "Devdocs",
    "appId": "com.gengjiawen.devdocs",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "mac": {
      "icon": "app/icons/icon.icns"
    },
    "win": {
      "icon": "app/icons/icon.ico"
    },
    "linux": {
      "icon": "app/icons",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "extraResources": [
      "app/src/renderer/assets"
    ]
  },
  "author": "Greg Holguin <simulatedgreg@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "chai": "^4.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.0",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.7.5",
    "electron-builder": "^19.22.1",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.1.0",
    "electron-rebuild": "^1.5.7",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-html": "^3.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "inject-loader": "^3.0.0-beta4",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "require-dir": "^0.3.1",
    "rimraf": "^2.6.1",
    "spectron": "^3.6.0",
    "style-loader": "^0.18.2",
    "tree-kill": "^1.1.0",
    "url-loader": "^0.5.8",
    "vue-hot-reload-api": "^2.0.11",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.4",
    "vue-style-loader": "^3.0.0",
    "vue-template-compiler": "^2.2.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^4.0.0"
  },
  "dependencies": {}
}
